<h2>form new post</h2>


<%= simple_form_for [ @category, @post ] do |f| %>
  <%= f.input :title,  { autocomplete: 'off' } %>
  <%= f.input :description, { autocomplete: 'off' } %>
  <br>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <br>
   <%= f.submit "Add photo", class: "btn btn-primary" %>
<% end %>


  <%= link_to 'Back to categories', categories_path(@category) %>
